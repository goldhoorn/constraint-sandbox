(defdomain network (


(:operator 
    (!startt ?r ?t) ;Signatur Head
    (and (depends ?r ?t) (fullfills ?t ?r) ) ;precondition
    () ;delete list
    ((running ?r) (requsts ?r ?t ) )
)

(:operator 
    (!start_root)
    (not (is-running root))
    ()
    ((is-running root) (depends root root) (requests root root ) )
)

))

